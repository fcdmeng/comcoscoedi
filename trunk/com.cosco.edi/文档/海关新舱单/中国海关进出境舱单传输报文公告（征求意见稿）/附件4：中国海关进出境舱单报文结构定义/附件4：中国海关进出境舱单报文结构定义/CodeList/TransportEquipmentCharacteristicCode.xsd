<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="TransportEquipmentCharacteristicCode" targetNamespace="TransportEquipmentCharacteristicCode" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:simpleType name="TransportEquipmentCharacteristicCode">
		<xs:annotation>
			<xs:documentation>集装箱尺寸和类型代码</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="4"/>
			<xs:pattern value="[A-Za-z0-9]{4}|[A-Za-z0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
