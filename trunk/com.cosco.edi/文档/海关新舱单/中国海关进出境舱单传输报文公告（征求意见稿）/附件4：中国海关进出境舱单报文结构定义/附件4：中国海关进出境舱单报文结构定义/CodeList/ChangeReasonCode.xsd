<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="ChangeReasonCode" targetNamespace="ChangeReasonCode" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:simpleType name="ChangeReasonCode">
