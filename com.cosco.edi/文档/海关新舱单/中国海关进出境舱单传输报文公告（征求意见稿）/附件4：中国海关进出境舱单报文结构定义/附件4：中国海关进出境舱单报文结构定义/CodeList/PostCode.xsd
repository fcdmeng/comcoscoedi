<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="PostCode" targetNamespace="PostCode" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:simpleType name="PostCode">
		<xs:annotation>
			<xs:documentation>邮政代码</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="6"/>
